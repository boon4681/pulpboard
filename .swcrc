{
    "jsc": {
        "parser": {
            "syntax": "typescript",
            "tsx": false
        },
        "target": "es2020",
        "loose": false,
        "minify": {
            "compress": false,
            "mangle": {
                "toplevel": false,
                "keep_classnames": false,
                "keep_fnames": false,
                "keep_private_props": false,
                "ie8": false,
                "safari10": false
            }
        }
    },
    "module": {
        "type": "commonjs",
        "strict": true,
        "strictMode": true,
        "lazy": false,
        "noInterop": false
    },
    "minify": false,
    "isModule": true
}